    <script>
      // Function to generate the event blocks/elements that will appear in the above code.
      function generateEventBlocks(jsonData) {
      const eventContainer = document.querySelector('#BRUH');
      
      jsonData.forEach(event => {
          const eventBlock = document.createElement('div');
          eventBlock.className = 'grid grid-cols-2 gap-[15px] mt-6';
          
          eventBlock.innerHTML = `
          <div id="LEFT-SIDE" class="col-start-1 col-end-2 bg-csc-gray bg-opacity-[0.06] rounded-lg p-3">

              <div id="date" class="text-right text-[#23d18b] font-tommy text-2xl">
                  
                  ${event.date}
              
              </div>

              <div id="title" class="text-right font-tommy text-3xl mb-[6px]">
                  
                  ${event.title}
              
              </div>

              <div id="image">
                  
                  <img class="w-full h-full object-hidden rounded-md" src="${event.image}">
              
              </div>

          </div>

          <div id=""RIGHT-SIDE" class="col-start-2 col-end-3 rounded-lg p-3">

              <div id="description" class="font-tommy text-2xl text-right">
                  
                  ${event.description}
                  
              </div>

          <div>
          `;
          
          eventContainer.appendChild(eventBlock);
      });
      }

    fetch('../events.json')
    .then(response => response.json())
    .then(data => {
        // Generate blocks for upcoming events
        generateEventBlocks(data.upcoming_events, '#BRUH');
    })
      .catch(error => console.error('Error fetching JSON:', error)); //not necessary, but at least we'll know if the JSON stuff don't work ¯\_(ツ)_/¯
    </script>

    <script>
        // Function to generate the event blocks/elements that will appear in the above code.
        function generateEventBlocks(jsonData) {
        const eventContainer = document.querySelector('#past-meet');
        
        jsonData.forEach(event => {
            const eventBlock = document.createElement('div');
            eventBlock.className = 'grid grid-cols-2 gap-[15px] mt-6';
            
            eventBlock.innerHTML = `
            <div id="LEFT-SIDE" class="col-start-1 col-end-2 bg-csc-gray bg-opacity-[0.06] rounded-lg p-3">

                <div id="date" class="text-right text-[#23d18b] font-tommy text-2xl">
                    
                    ${event.date}
                
                </div>

                <div id="title" class="text-right font-tommy text-3xl mb-[6px]">
                    
                    ${event.title}
                
                </div>

                <div id="image">
                    
                    <img class="w-full h-full object-hidden rounded-md" src="${event.image}">
                
                </div>

            </div>

            <div id="RIGHT-SIDE" class="col-start-2 col-end-3 rounded-lg p-3">

                <div id="description" class="font-tommy text-2xl text-right">
                    
                    ${event.description}
                    
                </div>

            <div>
            `;
            
            eventContainer.appendChild(eventBlock);
        });
        }

    fetch('../events.json')
    .then(response => response.json())
    .then(data => {      
        // Generate blocks for past events
        generateEventBlocks(data.past_events, '#past-meet');
    })
        .catch(error => console.error('Error fetching JSON:', error)); //not necessary, but at least we'll know if the JSON stuff don't work ¯\_(ツ)_/¯
    </script>